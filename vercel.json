{
    "builds": [
      {
        "src": "dist/service-worker.js",
        "use": "@vercel/static"
      },
      {
        "src": "dist/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/service-worker.js",
        "dest": "/dist/service-worker.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  